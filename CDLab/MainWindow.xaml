<Window
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:CDLab"
        xmlns:ObjectLayer="clr-namespace:ObjectLayer;assembly=ObjectLayer" x:Class="CDLab.MainWindow"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <ListBox HorizontalAlignment="Stretch" Margin="10,10,10,10" VerticalAlignment="Stretch">
            <ListBox.DataContext>
                <ObjectLayer:Computer/>
            </ListBox.DataContext>
        </ListBox>

    </Grid>
</Window>
